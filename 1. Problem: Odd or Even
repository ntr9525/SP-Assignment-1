#include<stdio.h>
int main()
{
    int n;
    printf("Enter the number: \n");
    scanf("%d",&n);  // To input number

    if(n%2==0) // Even numbers are divided by 0 and no remainder
    {
        printf("\"The number is Even\"\n");
    }
    else if (n%2!=0) //Remainder exists when odd is divided by 2
    {
        printf("\"The number is Odd\"\n");
    }
    else
    {
        return 0;
    }
    return 0;
}

