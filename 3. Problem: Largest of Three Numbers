#include<stdio.h>
int main()
{
    int x,y,z;
    printf("Enter the 1st number\n");
    scanf("%d",&x);
    printf("Enter the 2nd number\n");
    scanf("%d",&y);
    printf("Enter the 3rd number\n");
    scanf("%d",&z);
    if(x>=y)
    {
        if(x>=z)
        {
            printf("%d is the largest\n",x);
        }
        else
        {
            printf("%d is the largest\n",z);
        }
    }
    else if(y>=x)
    {
        if(y>=z)
        {
            printf("%d is the largest\n",y);
        }
        else
        {
            printf("%d is the largest\n",z);
        }
    }
    else if(z>=x)
    {
        if(z>=y)
        {
            printf("%d is the largest\n",z);
        }
        else
        {
            printf("%d is the largest\n",y);
        }
    }


    else
    {
        return 0;
    }
    return 0;
}
