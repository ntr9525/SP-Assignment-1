
#include<stdio.h>
int main()
{
    int n,digit=0;
    scanf("%d",&n);
    if(n>0)
    {


        while(n>0)
        {
            n= n/10;
            digit=digit+1;

        }
        printf("%d\n",digit);
    }
    else if(n==0)
    {
        printf("1\n");

    }
    else if(n<0)

    {
        n=-1*n;
        while(n>0)
        {
            n= n/10;
            digit=digit+1;

        }
        printf("%d\n",digit);

    }

    return 0;
}
