#include<stdio.h>
int main()
{
    int a,b,c;
    printf("Enter the length of 1st side:\n");
    scanf("%d",&a);
    printf("Enter the length of 2nd side:\n");
    scanf("%d",&b);
    printf("Enter the length of 3rd side:\n");
    scanf("%d",&c);
    if(a>0 && b>0 && c>0) //sides are positive
    {
        if(a+b > c && b+c>a  && a+c>b) // the sum of 2 sides> 3rd side
        {
            printf("Valid\n");
        }
        else
        {
            printf("Not Valid\n");
        }

    }
    else
    {
        printf("Not Valid\n");
    }


    return 0;
}

