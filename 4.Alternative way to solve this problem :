#include<stdio.h>
int main()
{

    int n;
    printf("Enter Mark\n");
    scanf("%d",&n);

    switch(n/10) //for example 100/10=10 so, 10=A
    {
    case 10:
    case 9:
    case 8:
        printf("A\n");
        break;
    case 7:
    case 6:
        printf("B\n");
        break;
    case 5:
        printf("C\n");
        break;
    case 4:
        printf("D\n");
        break;
    case 3:
    case 2:
    case 1:
    case 0:
        printf("F\n");
        break;
    default:
        printf("Invalid Input\n");

    }

    return 0;

}

