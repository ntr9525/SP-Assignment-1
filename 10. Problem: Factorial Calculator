#include<stdio.h>
int main()
{
    int n,fact=1,i=0;
    scanf("%d",&n);

    if (n>0)
    {

        while(i<n)
        {
            fact=fact*(n-i);
            i++;

        }
        printf("%d!=%d\n",n,fact);

    }
    else if (n==0)
    {
        printf("0!=1");
    }
    else
    {
        printf("invalid input\n");
    }
    return 0;
}
