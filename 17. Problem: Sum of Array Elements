#include<stdio.h>
int main()
{
    int n,sum=0;
    printf("Enter Size of Array:");
    scanf("%d",&n);

    int arr[n];
    printf("Enter the numbers:\n");
    for(int i=0; i<n; i++)
    {


        scanf("%d",&arr[i]);

    }

    for(int i=0; i<n; i++)
    {
        sum+=arr[i];
    }

    printf("the sum is:%d\n",sum);

return 0;
}
